{
  "expo": {
    "name": "Noteum",
    "slug": "Noteum",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [      
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#050505"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.oytu.noteum",
      "versionCode": 1,
      "googleServicesFile": "src/google-services.json",
      "config": {
        "googleSignIn": {
          "apiKey": "<key_created_in_google_cloud>",
          "certificateHash": "<hash_saved_in_google_cloud>"
        }
        
      }
    }
  }
}
