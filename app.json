{
  "expo": {
    "name": "Noteum",
    "slug": "Noteum",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [      
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#050505"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.noteum",
      "versionCode": 6,
      "googleServicesFile": "src/google-services.json",
      "config": {
        "googleSignIn": {
          "apiKey": "<API_KEY>", 
          "certificateHash": "<HASH>"
        }                
      },
      "adaptiveIcon":{
        "backgroundColor": "#000000",
        "foregroundImage":"./assets/adaptiveIcon.png"
      },
      "permissions": [
        "CAMERA",
        "READ_PHONE_STATE",
        "RECORD_AUDIO",
        "READ_CONTACTS"
      ]
    },
    "scheme":"noteum"
    
  }
}
